<%= form_for([current_user, @interview]) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.label :start_time, '開始時間' %><br />
    <%= f.datetime_select :start_time, default: Time.now + 1.week %>
  </div>
  <br />
  <div class="actions">
    <%= f.submit button, class: "btn btn-primary" %>
  </div>
<% end %>
